pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    
    
    
        stage('Build') {
            steps {
                echo 'Build World'
            }
        }
    
    
    
        stage('Confirm') {
            steps {
                echo 'Confirm World'
            }
        }
    
    
    
        stage('All Good') {
            steps {
                echo 'All Good World'
            }
        }
    }
}
